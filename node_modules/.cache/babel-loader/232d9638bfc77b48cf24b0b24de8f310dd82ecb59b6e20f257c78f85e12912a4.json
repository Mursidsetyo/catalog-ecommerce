{"ast":null,"code":"export default {\n  data() {\n    return {\n      products: [],\n      index: 1,\n      loading: true,\n      loadingImage: require(\"../assets/hourglass.gif\")\n    };\n  },\n  methods: {\n    async fetchData() {\n      const response = await fetch(\"https://fakestoreapi.com/products/\" + this.index);\n      const data = await response.json();\n      this.loading = false;\n      this.products = data;\n    },\n    btnNext() {\n      if (this.index < 20) {\n        this.index++;\n        this.fetchData();\n      } else {\n        this.index = 1;\n      }\n      console.log(this.index);\n    }\n  },\n  created() {\n    this.loading = true;\n    this.fetchData();\n  }\n};","map":{"version":3,"mappings":"AAkIE,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CAAC;MACRC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAEC,OAAO,CAAC,yBAAyB;IACjD,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAAS,GAAG;MAChB,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAC1B,oCAAmC,GAAI,IAAI,CAACP,KAAI,CACjD;MACD,MAAMF,IAAG,GAAI,MAAMQ,QAAQ,CAACE,IAAI,EAAE;MAClC,IAAI,CAACP,OAAM,GAAI,KAAK;MACpB,IAAI,CAACF,QAAO,GAAID,IAAI;IACtB,CAAC;IACDW,OAAO,GAAG;MACR,IAAI,IAAI,CAACT,KAAI,GAAI,EAAE,EAAE;QACnB,IAAI,CAACA,KAAK,EAAE;QACZ,IAAI,CAACK,SAAS,EAAE;MAClB,OAAO;QACL,IAAI,CAACL,KAAI,GAAI,CAAC;MAChB;MACAU,OAAO,CAACC,GAAG,CAAC,IAAI,CAACX,KAAK,CAAC;IACzB;EACF,CAAC;EACDY,OAAO,GAAG;IACR,IAAI,CAACX,OAAM,GAAI,IAAI;IACnB,IAAI,CAACI,SAAS,EAAE;EAClB;AACF,CAAC","names":["data","products","index","loading","loadingImage","require","methods","fetchData","response","fetch","json","btnNext","console","log","created"],"sourceRoot":"","sources":["C:\\Users\\Mursid\\Desktop\\ecommerce\\src\\components\\ProductDisplay.vue"],"sourcesContent":["<template>\r\n    <div\r\n      v-if=\"!loading\"\r\n      :class=\"\r\n        products.category == 'men\\'s clothing'\r\n          ? 'container-men'\r\n          : 'container-women' && products.category == 'women\\'s clothing'\r\n          ? 'container-women'\r\n          : 'container-men' && products.category == 'electronics'\r\n          ? 'container-unvailable'\r\n          : 'container-unvailable'\r\n      \"\r\n    >\r\n      <section\r\n        v-if=\"\r\n          products.category == 'men\\'s clothing' ||\r\n          products.category == 'women\\'s clothing'\r\n        \"\r\n      >\r\n        <div class=\"card\">\r\n          <div class=\"flex-wrap\">\r\n            <div class=\"left\">\r\n              <img :src=\"products.image\" alt=\"\" />\r\n            </div>\r\n            <div class=\"right\">\r\n              <h2\r\n                :class=\"\r\n                  products.category == 'men\\'s clothing'\r\n                    ? 'title-men'\r\n                    : 'title-women'\r\n                \"\r\n              >\r\n                {{ products.title }}\r\n              </h2>\r\n              <div class=\"rating-category\">\r\n                <p class=\"category\">{{ products.category }}</p>\r\n                <div class=\"rating\">\r\n                  <p>{{ products.rating.rate }}/5.0</p>\r\n                  <span\r\n                    :class=\"\r\n                      products.category == 'men\\'s clothing'\r\n                        ? 'dot-men'\r\n                        : 'dot-women'\r\n                    \"\r\n                  ></span>\r\n                  <span\r\n                    :class=\"\r\n                      products.category == 'men\\'s clothing'\r\n                        ? 'dot-men'\r\n                        : 'dot-women'\r\n                    \"\r\n                  ></span>\r\n                  <span\r\n                    :class=\"\r\n                      products.category == 'men\\'s clothing'\r\n                        ? 'dot-men'\r\n                        : 'dot-women'\r\n                    \"\r\n                  ></span>\r\n                  <span\r\n                    :class=\"\r\n                      products.category == 'men\\'s clothing'\r\n                        ? 'dot-men-empty'\r\n                        : 'dot-women-empty'\r\n                    \"\r\n                  ></span>\r\n                  <span\r\n                    :class=\"\r\n                      products.category == 'men\\'s clothing'\r\n                        ? 'dot-men-empty'\r\n                        : 'dot-women-empty'\r\n                    \"\r\n                  ></span>\r\n                </div>\r\n              </div>\r\n              <hr style=\"width: 100%; text-align: left; margin-left: 0\" />\r\n              <p class=\"description\">{{ products.description.slice(0, 150) }}</p>\r\n              <hr style=\"width: 100%; text-align: left; margin-left: 0\" />\r\n              <h3\r\n                :class=\"\r\n                  products.category == 'men\\'s clothing'\r\n                    ? 'price-men'\r\n                    : 'price-women'\r\n                \"\r\n              >\r\n                ${{ products.price }}\r\n              </h3>\r\n              <div class=\"button\">\r\n                <button\r\n                  :class=\"\r\n                    products.category == 'men\\'s clothing'\r\n                      ? 'buy-btn-men'\r\n                      : 'buy-btn-women'\r\n                  \"\r\n                >\r\n                  Buy now\r\n                </button>\r\n                <button\r\n                  :class=\"\r\n                    products.category == 'men\\'s clothing'\r\n                      ? 'next-btn-men'\r\n                      : 'next-btn-women'\r\n                  \"\r\n                  @click.prevent=\"btnNext()\"\r\n                >\r\n                  Next product\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n      <section v-else>\r\n        <div class=\"card\">\r\n          <img class=\"unvailable-img\" src=\"@/assets/unvailable.png\" alt=\"\" />\r\n          <div class=\"description-unvailable\">\r\n            <h2>This product is unvailable to show</h2>\r\n            <button class=\"unvailable-btn\" @click.prevent=\"btnNext()\">\r\n              Next product\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </div>\r\n    <div class=\"loading\" v-else>\r\n      <div class=\"loader\"></div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        products: [],\r\n        index: 1,\r\n        loading: true,\r\n        loadingImage: require(\"../assets/hourglass.gif\"),\r\n      };\r\n    },\r\n    methods: {\r\n      async fetchData() {\r\n        const response = await fetch(\r\n          \"https://fakestoreapi.com/products/\" + this.index\r\n        );\r\n        const data = await response.json();\r\n        this.loading = false;\r\n        this.products = data;\r\n      },\r\n      btnNext() {\r\n        if (this.index < 20) {\r\n          this.index++;\r\n          this.fetchData();\r\n        } else {\r\n          this.index = 1;\r\n        }\r\n        console.log(this.index);\r\n      },\r\n    },\r\n    created() {\r\n      this.loading = true;\r\n      this.fetchData();\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style></style>\r\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}